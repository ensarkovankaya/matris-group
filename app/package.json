{
  "name": "matris-group",
  "version": "0.0.1",
  "description": "Group API",
  "main": "index.js",
  "author": "Ensar Kovankaya <ensar@kovankaya.com>",
  "license": "ISC",
  "scripts": {
    "test": "mocha -b -r ts-node/register test/unit/**/*.spec.ts test/unit/**/**/*.spec.ts test/unit/**/**/**/*.spec.ts",
    "test:data": "mocha -b -r ts-node/register test/data/**/*.spec.ts",
    "e2e": "mocha -b -r ts-node/register test/e2e/**/*.spec.ts",
    "check": "tslint -p tslint.json",
    "build": "npm run check && tsc",
    "build:clean": "rm -rf ./build/* && npm run build",
    "start:prod": "node ./build/index.js",
    "start:test": "nodemon -e ts -w src/ -i test/ --exec 'ts-node test/server/index.ts'",
    "start:dev": "nodemon -e ts -w src/ -i test/ --exec 'ts-node src/index.ts'"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "matris-account-api": "^1.4.0",
    "matris-logger": "^1.1.5",
    "mongoose": "^5.2.4",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "slugify": "^1.3.1",
    "type-graphql": "^0.12.3",
    "typedi": "^0.7.3",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "^0.13.3",
    "@types/helmet": "0.0.38",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.0",
    "@types/mongoose-paginate": "^5.0.6",
    "@types/morgan": "^1.7.35",
    "@types/validator": "^9.4.1",
    "chai": "^4.1.2",
    "graphql-request": "^1.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "ts-node": "^6.1.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
